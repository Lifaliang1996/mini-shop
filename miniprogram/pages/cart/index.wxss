page {
  height: 100vh;
  padding-bottom: 50px;
}
#goods-list {
  height: 100%;
}
#goods-list .goods-card {
  width: 100%;
  height: 220rpx;
  padding: 20rpx 30rpx;
}
#goods-list .goods-card:not(:last-child) {
  border-bottom: 1px solid #f8f8f8;
}
#goods-list .goods-card .warp {
  display: flex;
  align-items: center;
  width: 100%;
  height: 180rpx;
}
#goods-list .goods-card .warp .check-box {
  padding-right: 10rpx;
}
#goods-list .goods-card .warp .img {
  height: 180rpx;
  width: 180rpx;
  margin-right: 20rpx;
  border-radius: 20rpx;
  overflow: hidden;
}
#goods-list .goods-card .warp .img .van-image {
  width: 100%;
  height: 100%;
}
#goods-list .goods-card .warp .info {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  height: 100%;
}
#goods-list .goods-card .warp .info .title {
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 28rpx;
}
#goods-list .goods-card .warp .info .price-num-wrap {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}
.van-submit-bar {
  border-top: 1px solid #f8f8f8;
}
